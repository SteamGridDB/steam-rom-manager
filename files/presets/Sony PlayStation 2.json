      "xbox360": null,
      "xboxone": null,
      "switch_joycon_left": null,
      "switch_joycon_right": null,
      "switch_pro": null,
      "neptune": null
    },
    "defaultImage": {
      "long": "",
      "tall": "",
      "hero": "",
      "logo": "",
      "icon": ""
    },
    "localImages": {
      "long": "",
      "tall": "",
      "hero": "",
      "logo": "",
      "icon": ""
    },
    "steamInputEnabled": "1",
    "drmProtect": false,
    "steamCategories": ["PS2"]
