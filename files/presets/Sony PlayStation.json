			"specifiedAccounts": "",
			"skipWithMissingDataDir": true,
			"useCredentials": true
		},
		"parserInputs": {
			"glob": "${title}@(.bin|.BIN|.ccd|.CCD|.cue|.CUE|.ecm|.ECM|.iso|.ISO|.mds|.MDS|.pbp|.PBP)",
			"glob-regex": null
		},
